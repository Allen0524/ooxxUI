{
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": true,
			"decorators": false,
			"dynamicImport": true
		},
		"transform": {
			"react": {
				"runtime": "automatic"
			}
		},
		"target": "es5",
		"loose": false,
		"externalHelpers": false,
		// Requires v1.2.50 or upper and requires target to be es2016 or upper.
		"keepClassNames": false
	},
	"module": {
		"type": "es6",

		// These are defaults.
		"strict": false,
		"strictMode": true,
		"lazy": false,
		"noInterop": false
	},
	"sourceMaps": true,
	"minify": false
}
